<body class="hold-transition login-page">
    <div class="login-box">
    <div class="login-box-body">
        <h3 class="login-box-msg">Register As Event Manager Organization</h3>
                <form class="form-horizontal" asp-action="RegisterNewUser" asp-controller="Account" method="post">
                    <div class="form-group has-feedback">
                            <input type="text" asp-for="UserName" placeholder="User Name" class="form-control sty1" autocomplete="off" />
                            <span asp-validation-for="UserName" class="text-danger"></span>
                    </div>
                    <div class="form-group has-feedback">
                            <input type="text" asp-for="PhoneNumber" placeholder="Primary Contact Number" class="form-control sty1" autocomplete="off" />
                            <span asp-validation-for="PhoneNumber" class="text-danger"></span>
                    </div>
                    <div class="form-group has-feedback">
                            <input asp-for="Email" class="form-control sty1" placeholder="Registration mail Id (used for login)" autocomplete="off" />
                            <span asp-validation-for="Email" class="text-danger"></span>
                   </div>
                    <div class="form-group has-feedback">
                            <input asp-for="Password" class="form-control sty1" type="password" placeholder="Enter password" autocomplete="off" />
                            <span asp-validation-for="Password" class="text-danger"></span>
                    </div>
                    <div class="form-group has-feedbackp">
                            <input asp-for="ConfirmPassword" class="form-control sty1" type="password" placeholder="Re Enter password" autocomplete="off" />
                            <span asp-validation-for="ConfirmPassword" class="text-danger"></span>
                    </div>

                <div class="text-center">
                            <button type="submit" class="btn btn-primary" >SignUp </button>
                            <div asp-validation-summary="All" class="text-danger"></div>
                    </div>

                </form>
            <div class="col-xs-4 m-t-1 text-center">
                Already have an account?
                <a href="/account/RegisterNewUser" class="link btn-link"> <a asp-controller="Account" asp-action="Login"> Login</a>
            </div>
       <main role="main" class="pb-3">
            <div class="row">
                <div class="col-md-4">
                    <section>
                    </section>
                </div>
            </div>
        </main>
        </div>
    </div>
   <script src="~/uitemplate/admintemp/dist/js/jquery.min.js"></script>
    <script src="~/uitemplate/admintemp/dist/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/uitemplate/admintemp/dist/js/niche.html"></script>
</body>

</html>






